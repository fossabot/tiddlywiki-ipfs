{
	"description": "Empty edition",
	"plugins": [
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"empty": [
			"--rendertiddler", "$:/core/save/all", "empty.html", "text/plain"
		],
    "plugin": [
      "--render",
        "$:/plugins/ipfs",
        "tiddlywiki-ipfs-plugin.json",
        "text/plain",
        "$:/core/templates/json-tiddler",
      "--render",
        "[tag[$:/ipfs/core]]",
        "[encodeuricomponent[]addprefix[./]addsuffix[.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ]
	}
}