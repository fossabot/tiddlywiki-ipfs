{
	"description": "Empty edition",
	"plugins": [
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"empty": [
			"--compress", "true",
			"--rendertiddler", "$:/core/save/all", "empty.html", "text/plain"
		],
		"empty-encrypted": [
			"--compress", "true",
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../sample/empty-encrypted.html", "text/plain",
      "--clearpassword"
    ],
		"empty-compressed": [
			"--compress", "true",
			"--rendertiddler", "$:/core/save/all", "../sample/empty-plain.html", "text/plain"
		],
		"empty-plain": [
			"--compress", "false",
			"--rendertiddler", "$:/core/save/all", "../sample/empty-plain.html", "text/plain"
		],
		"empty-plain-and-encrypted": [
			"--compress", "false",
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../sample/empty-plain-and-encrypted.html", "text/plain",
      "--clearpassword"
    ],
    "plugin": [
      "--render",
        "[all[shadows+tiddlers]tag[$:/ipfs/core]]",
        "tiddlywiki-ipfs-plugin.json",
        "text/plain",
        "$:/core/templates/json-tiddler",
      "--render",
        "[tag[$:/ipfs/core]]",
        "[encodeuricomponent[]addprefix[./]addsuffix[.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ]
	}
}