{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"index": [
			"--compress", "true",
			"--rendertiddler", "$:/core/save/all", "index.html", "text/plain"
    ],
		"index-encrypted": [
			"--compress", "true",
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../sample/index-encrypted.html", "text/plain",
      "--clearpassword"
    ],
    "index-plain": [
      "--rendertiddler", "$:/core/save/all", "../sample/index-plain.html", "text/plain"
    ],
		"index-plain-and-encrypted": [
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../sample/index-plain-and-encrypted.html", "text/plain",
      "--clearpassword"
    ]
  }
}
