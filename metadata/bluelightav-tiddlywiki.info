{
  "plugins": [
    "tiddlywiki/browser-sniff",
    "tiddlywiki/codemirror",
    "tiddlywiki/codemirror-closebrackets",
    "tiddlywiki/codemirror-closetag",
    "tiddlywiki/codemirror-fullscreen-editing",
    "tiddlywiki/codemirror-mode-css",
    "tiddlywiki/codemirror-mode-htmlembedded",
    "tiddlywiki/codemirror-mode-htmlmixed",
    "tiddlywiki/codemirror-mode-javascript",
    "tiddlywiki/codemirror-mode-markdown",
    "tiddlywiki/codemirror-mode-xml",
    "tiddlywiki/codemirror-search-replace",
    "tiddlywiki/highlight",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"index": [
			"--compress", "true",
			"--rendertiddler", "$:/core/save/all", "index.html", "text/plain"
    ],
		"index-encrypted": [
			"--compress", "true",
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../output/index-encrypted.html", "text/plain",
      "--clearpassword"
    ],
    "index-plain": [
      "--rendertiddler", "$:/core/save/all", "../output/index-plain.html", "text/plain"
    ],
		"index-plain-and-encrypted": [
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../output/index-plain-and-encrypted.html", "text/plain",
      "--clearpassword"
    ],
    "plugin": [
      "--render", "$:/plugins/ipfs", "tiddlywiki-ipfs-plugin.json", "text/plain", "$:/core/templates/json-tiddler"
    ]
  }
}
